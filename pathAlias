alias l="ls -la"
alias projects="cd ~/Documents/projects"
alias iosCache="cd ~/Library/Developer/Xcode/DerivedData"
alias bashf="cd ~/bash"



path_alias_help(){
echo "
alias l="ls -la"
alias projects="cd ~/Documents/projects"
alias iosCache="cd ~/Library/Developer/Xcode/DerivedData"
alias bashf="cd ~/bash"
" 
    
}
